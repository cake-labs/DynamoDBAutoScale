<?xml version="1.0"?>
<configuration>
	<configSections>
		<section name ="Rules" type ="DynamoDBAutoScale.ConfigMapping.XMLSerializerSectionHandler, DynamoDBAutoScale"/>
	</configSections>
	<Rules configSource="Rules.config" />
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>
	<appSettings>
		<add key="AWSAccessKeyID" value="#aws_access_key_id#" />
		<add key="AWSSecretAccessKey" value="#aws_secret_access_key#" />
		<add key="AWSRegionEndpoint" value="#aws_region_endpoint#" />
	</appSettings>
</configuration>